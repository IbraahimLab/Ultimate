{
  "name": "Ultimate",
  "version": "0.1.0",
  "description": "MVP coding agent CLI with plan/act/verify and diff approvals",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "vibe-agent": "dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc -p tsconfig.json",
    "check": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/cli.js",
    "pack:dry": "npm pack --dry-run",
    "build:py": "python -m build packages/python-sdk"
  },
  "dependencies": {
    "diff": "^7.0.0",
    "dotenv": "^17.2.3",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/diff": "^7.0.1",
    "@types/node": "^24.0.1",
    "tsx": "^4.20.6"
  },
  "engines": {
    "node": ">=20"
  }
}
